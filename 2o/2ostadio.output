Grammar

    0 $accept: program "End Of File"

    1 program: header declarations subprograms comp_statement "."

    2 header: "Program" "Id" ";"

    3 declarations: constdefs typedefs vardefs

    4 constdefs: "Const" constant_defs ";"
    5          | %empty

    6 constant_defs: constant_defs ";" "Id" "=" expression
    7              | "Id" "=" expression
    8              | "Id" error

    9 expression: expression "> | >= | < | <= | <>" expression
   10           | expression "=" expression
   11           | expression "In" expression
   12           | expression "Or" expression
   13           | expression "+ | -" expression
   14           | expression "* | / | Mod | Div | And" expression
   15           | "+ | -" expression
   16           | "Not" expression
   17           | variable
   18           | "Id" "(" expressions ")"
   19           | "Length" "(" expression ")"
   20           | "New" "(" expression ")"
   21           | constant
   22           | "(" expression ")"
   23           | setlistexpression

   24 variable: "Id"
   25         | variable "." "Id"
   26         | variable "[" expressions "]"
   27         | "Listfunction" "(" expression ")"

   28 expressions: expressions "," expression
   29            | expression

   30 constant: "Iconst"
   31         | "Rconst"
   32         | "Bconst"
   33         | "Cconst"

   34 setlistexpression: "[" expressions "]"
   35                  | "[" "]"

   36 typedefs: "Type" type_defs ";"
   37         | %empty

   38 type_defs: type_defs ";" "Id" "=" type_def
   39          | "Id" "=" type_def

   40 type_def: "Array" "[" dims "]" "Of" typename
   41         | "List" "Of" typename
   42         | "Set" "Of" typename
   43         | "Record" fields "End"
   44         | limit ".." limit

   45 dims: dims "," limits
   46     | limits

   47 limits: limit ".." limit
   48       | "Id"

   49 limit: sign "Iconst"
   50      | "Cconst"
   51      | "Bconst"
   52      | "+ | -" "Id"
   53      | "Id"

   54 sign: "+ | -"
   55     | %empty

   56 typename: standard_type
   57         | "Id"

   58 standard_type: "Integer"
   59              | "Real"
   60              | "Boolean"
   61              | "Char"

   62 fields: fields ";" field
   63       | field

   64 field: identifiers ":" typename

   65 identifiers: identifiers "," "Id"
   66            | "Id"
   67            | error

   68 vardefs: "Var" variable_defs ";"
   69        | %empty

   70 variable_defs: variable_defs ";" identifiers ":" typename
   71              | identifiers ":" typename

   72 $@1: %empty

   73 subprograms: subprograms $@1 subprogram ";"
   74            | %empty

   75 subprogram: sub_header ";" "Forward"
   76           | sub_header ";" declarations subprograms comp_statement
   77           | sub_header ";" error

   78 sub_header: "Function" "Id" formal_parameters ":" standard_type
   79           | "Function" "Id" formal_parameters ":" "List"
   80           | "Procedure" "Id" formal_parameters
   81           | "Function" "Id"

   82 formal_parameters: "(" parameter_list ")"
   83                  | %empty

   84 parameter_list: parameter_list ";" pass identifiers ":" typename
   85               | pass identifiers ":" typename

   86 pass: "Var"
   87     | %empty

   88 comp_statement: "Begin" statements "End"

   89 statements: statements ";" statement
   90           | statement

   91 statement: assignment
   92          | if_statement
   93          | case_statement
   94          | while_statement
   95          | for_statement
   96          | with_statement
   97          | subprogram_call
   98          | io_statement
   99          | comp_statement
  100          | %empty

  101 assignment: variable ":=" expression
  102           | variable ":=" "String"

  103 $@2: %empty

  104 if_statement: "If" expression "Then" $@2 statement if_tail
  105             | "If" expression error

  106 $@3: %empty

  107 if_tail: "Else" $@3 statement
  108        | %empty

  109 case_statement: "Case" expression "Of" cases case_tail "End"

  110 cases: cases ";" single_case
  111      | single_case

  112 single_case: label_list ":" statement
  113            | %empty

  114 label_list: label_list "," label
  115           | label

  116 label: sign constant
  117      | sign "Id"

  118 case_tail: ";" "Otherwise" ":" statement
  119          | %empty

  120 $@4: %empty

  121 while_statement: "While" expression "Do" $@4 statement

  122 $@5: %empty

  123 $@6: %empty

  124 for_statement: "For" "Id" ":=" iter_space "Do" $@5 statement $@6
  125              | "For" error

  126 iter_space: expression "To" expression
  127           | expression "Downto" expression

  128 with_statement: "With" variable "Do" statement

  129 subprogram_call: "Id"
  130                | "Id" "(" expressions ")"

  131 io_statement: "Read" "(" read_list ")"
  132             | "Write" "(" write_list ")"

  133 read_list: read_list "," read_item
  134          | read_item

  135 read_item: variable

  136 write_list: write_list "," write_item
  137           | write_item

  138 write_item: expression
  139           | "String"


